{
    "name": "webparking/phpstan-lumen",
    "license": "BSD-2-Clause",
    "description": "Lumen plugins for PHPStan",
    "type": "library",
    "authors": [
        {
            "name": "Chris Leppanen",
            "email": "chris.leppanen@weebly.com"
        },
        {
            "name": "Jeroen van Oort",
            "email": "jeroen.vanoort@webparking.nl"
        }
    ],
    "require": {
        "laravel/lumen-framework": "5.5.* || 5.6.*",
        "phpstan/phpstan": "^0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5.2"
    },
    "autoload": {
        "psr-4": {
            "Webparking\\PHPStan\\Lumen\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Webparking\\PHPStan\\Lumen\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
